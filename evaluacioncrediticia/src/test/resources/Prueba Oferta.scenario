<scenario>
  <name>Prueba Oferta.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>oferta_prospecto</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>Producto</type>
      <fieldData>
        <field-data>
          <name>target</name>
          <value>3</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$producto</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Prospecto</type>
      <fieldData>
        <field-data>
          <name>nivelCliente</name>
          <value>3</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>maxEndeudamiento</name>
          <value>1000</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$prospecto</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Oferta para Prospectos</ruleName>
      <expectedCount>1</expectedCount>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>producto</fieldName>
          <expected>=$producto</expected>
          <operator>==</operator>
          <nature>2</nature>
        </expect-field>
        <expect-field>
          <fieldName>prospecto</fieldName>
          <expected>=$prospecto</expected>
          <operator>==</operator>
          <nature>2</nature>
        </expect-field>
        <expect-field>
          <fieldName>montoMaximo</fieldName>
          <expected>-1</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>Oferta</name>
      <anonymous>true</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.evaluacioncrediticia.Oferta</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.evaluacioncrediticia.Prospecto</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.evaluacioncrediticia.Producto</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>